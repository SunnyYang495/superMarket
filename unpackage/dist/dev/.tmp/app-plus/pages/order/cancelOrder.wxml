<view class="cart commonView"><view class="shopName flexBox"><view data-event-opts="{{[['tap',[['selectAll',['$event']]]]]}}" class="iconView" bindtap="__e"><label class="{{['iconfont icon-success _span',[(isAllSelect)?'selectIcon':'']]}}"></label></view><view class="cartCont">{{shopName}}</view></view><view class="goodsList"><block wx:for="{{cancelList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods flexBox"><view data-event-opts="{{[['tap',[['selectedGoods',[index]]]]]}}" class="iconView" bindtap="__e"><label class="{{['iconfont icon-success _span',[(item.isSelected)?'selectIcon':'']]}}"></label></view><view class="cartCont flexBox"><view class="goodsImg"><image src="{{item.cover}}" class="_img"></image></view><view class="goodsInfo"><view class="textOver">{{''+item.Goods_name+''}}</view><view class="price">{{'￥'+item.Goods_price+''}}</view></view></view><view class="edit"><view>{{'×'+item.Goods_count+''}}</view></view></view></block></view><view class="flexBox settleView"><view class="deliveryPrice">起送价:￥0.0</view><view class="totalMoney">合计:<text class="price">{{"￥"+totalMoney}}</text></view><view data-event-opts="{{[['tap',[['cancelOrder',['$event']]]]]}}" class="settle" bindtap="__e">退货</view></view></view>