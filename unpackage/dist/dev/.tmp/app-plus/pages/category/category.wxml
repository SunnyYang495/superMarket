<view class="flexBox categoryView"><view class="categoryList"><block wx:for="{{categoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getSubCate',[index]]]]]}}" class="category" bindtap="__e"><text class="{{[[(item.isClicked)?'cateClicked':'']]}}">{{item.Category_name}}</text></view></block></view><view class="subCategorys flexBox"><block wx:for="{{subCategoryList}}" wx:for-item="subitem" wx:for-index="subIndex" wx:key="subIndex"><view data-event-opts="{{[['tap',[['gotoGoodsList',[subIndex]]]]]}}" class="subCate" bindtap="__e"><view class="cateImg"><image src="{{subitem.cover}}" alt class="_img"></image></view><view class="cateName">{{subitem.Category_name}}</view></view></block></view></view>