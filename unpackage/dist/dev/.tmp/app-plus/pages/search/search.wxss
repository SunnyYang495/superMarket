
.search{
	height: 80rpx;
	line-height: 60rpx;
	background:#DADADA;
	padding:10px 10px 0;
}
.searchInput{
	background: #fff;
	width:70%;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
}
.searchIcon ._span{
	font-size: 40rpx;
	color:#8F8F94;
}
.searchBtn{
	width:30%;
	padding-left: 10px;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.searchBtn button{
	background: #3AA7F8;
	color:#fff;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
	text-align: center;
	height: 60rpx;
	line-height: 60rpx;
}

